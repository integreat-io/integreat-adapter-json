{
  "name": "integreat-adapter-json",
  "version": "0.3.4",
  "description": "JSON adapter for Integreat",
  "keywords": [
    "json",
    "integreat"
  ],
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "test": "npm run lint && tsc && nyc --reporter=text-summary ava dist/**/*.test.js",
    "test:watch": "npm run dev",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "dev": "ava --watch src/**/*.test.ts",
    "build": "tsc",
    "prepare": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "dist/**/*.test.js",
      "src/**/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat-adapter-json.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat-adapter-json/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat-adapter-json#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.4"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/got": "^9.6.9",
    "debug": "^4.1.1",
    "got": "^10.2.2",
    "great-uri-template": "^0.1.5"
  },
  "devDependencies": {
    "@types/sinon": "^9.0.0",
    "ava": "^3.7.1",
    "coveralls": "^3.0.11",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-security": "^1.16.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.8.3"
  }
}
