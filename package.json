{
  "name": "integreat-adapter-json",
  "version": "0.1.6",
  "description": "JSON adapter for Integreat",
  "keywords": [
    "json",
    "integreat"
  ],
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "test": "npm run lint && tsc && nyc --reporter=text-summary ava dist/**/*.test.js",
    "test:watch": "npm run dev",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "dev": "ava --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "sources": [
      "src/**/!(*.test).ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat-adapter-json.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat-adapter-json/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat-adapter-json#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.4"
  },
  "dependencies": {
    "@types/debug": "^4.1.4",
    "@types/got": "^9.6.4",
    "debug": "^4.1.1",
    "got": "^9.6.0",
    "great-uri-template": "^0.1.2"
  },
  "devDependencies": {
    "@types/nock": "^10.0.3",
    "@types/sinon": "^7.0.13",
    "ava": "^2.2.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.5",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
